void setup()
{
 pinMode(13, OUTPUT);
}

void loop()
{
 digitalWrite(13, HIGH); //J
 delay(500);
 digitalWrite(13, LOW);
 delay(500);
 digitalWrite(13, HIGH);
 delay(1500);
 digitalWrite(13, LOW);
 delay(500);
 digitalWrite(13, HIGH);
 delay(1500);
 digitalWrite(13, LOW);
 delay(500);
 digitalWrite(13, HIGH);
 delay(1500);
 digitalWrite(13, LOW);
 delay(1500);
 digitalWrite(13, HIGH); //O
 delay(1500);
 digitalWrite(13, LOW);
 delay(500);
 digitalWrite(13, HIGH);
 delay(1500);
 digitalWrite(13, LOW);
 delay(500);
 digitalWrite(13, HIGH);
 delay(1500);
 digitalWrite(13, LOW);
 delay(1500);
 digitalWrite(13, HIGH); //B
 delay(1500);
 digitalWrite(13, LOW);
 delay(500);
 digitalWrite(13, HIGH);
 delay(500);
 digitalWrite(13, LOW);
 delay(500);
 digitalWrite(13, HIGH);
 delay(500);
 digitalWrite(13, LOW);
 delay(500);
 digitalWrite(13, HIGH);
 delay(500);
 digitalWrite(13, LOW);
 delay(1500);
 delay(8500);
}
